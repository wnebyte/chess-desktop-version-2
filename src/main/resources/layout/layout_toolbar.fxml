<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import org.kordamp.ikonli.javafx.FontIcon?>

<ToolBar xmlns="http://javafx.com/javafx"
         xmlns:fx="http://javafx.com/fxml"
         fx:id="toolBar" styleClass="toolBar">

    <Region styleClass="spacer" />
    <Button fx:id="homeButton" id="home-button" styleClass="first" prefWidth="50">
        <tooltip><Tooltip text="Home"/></tooltip>
        <graphic>
            <FontIcon iconLiteral="mdi2h-home" iconSize="16"/>
        </graphic>
    </Button>
    <Button fx:id="backButton" id="back-button" styleClass="last" prefWidth="50">
        <tooltip><Tooltip text="Back"/></tooltip>
        <graphic>
            <FontIcon iconLiteral="mdi2a-arrow-left-bold" iconSize="16"/>
        </graphic>
    </Button>

</ToolBar>
